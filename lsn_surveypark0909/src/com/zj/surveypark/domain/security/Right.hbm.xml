<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zj.surveypark.domain.security">
	<class name="Right" table="rights">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property name="rightUrl" column="righturl" type="java.lang.String" length="50" />
		<property name="rightName" column="rightname" type="java.lang.String" length="200" />
		<property name="rightDesc" column="rightdesc" type="java.lang.String" length="200" />
		<property name="rightPos" column="rightpos" type="java.lang.Integer" update="false"/>
		<property name="rightCode" column="rightcode" type="long" update="false" />
	</class>
</hibernate-mapping>